{
  "name": "solana-whale-tracker",
  "version": "1.0.0",
  "description": "Real-time Solana whale wallet tracker and monitoring system",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "scrape": "ts-node src/scraper.ts",
    "monitor": "ts-node src/monitor.ts",
    "wallet-manager": "ts-node src/walletManager.ts",
